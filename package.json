{
  "name": "normal-forme",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Elegant form components for React",
  "author": "Jimmy Sanford <jimmy.sanford.code@gmail.com>",
  "main": "dist/normal-forme.cjs.js",
  "module": "dist/normal-forme.esm.js",
  "sideEffects": false,
  "scripts": {
    "build": "rollup --config",
    "build:dev": "rollup --config --watch",
    "format": "prettier --write \"./**/*.js\"",
    "format:ci": "prettier --list-different \"./**/*.js\"",
    "lint": "eslint --config \"eslint.config.yml\" \".\"",
    "prepublishOnly": "yarn build",
    "test": "jest --coverage",
    "test:dev": "jest --coverage --verbose"
  },
  "peerDependencies": {
    "react": "16.7.0-alpha.2",
    "react-dom": "16.7.0-alpha.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.11.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^1.6.0",
    "jest": "^23.6.0",
    "jest-junit": "^5.2.0",
    "prettier": "^1.15.3",
    "react": "16.7.0-alpha.2",
    "react-dom": "16.7.0-alpha.2",
    "react-test-renderer": "16.7.0-alpha.2",
    "rollup": "^0.68.1",
    "rollup-plugin-babel": "^4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jimbo/normal-forme.git"
  },
  "keywords": [
    "react",
    "hooks",
    "form",
    "context"
  ],
  "files": [
    "dist"
  ]
}
